#include <iostream>

using namespace std;

int main() 
{
    int dollar = 0, cent = 0, cake = 0, totaldollar = 0, totalcent = 0;
    cin >> dollar >> cent >> cake;
    totalcent = cake * (dollar * 100 + cent) % 100;
    totaldollar = cake * (dollar * 100 + cent) / 100;
    cout << totaldollar << " " << totalcent;
    return 0;
}
